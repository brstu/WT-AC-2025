# Application Configuration
VITE_API_URL=http://localhost:3001
VITE_APP_NAME="Book Catalog"
VITE_APP_VERSION=1.0.0

# Docker Configuration
DOCKER_USERNAME=yourusername
DOCKER_PASSWORD=yourpassword
DOCKER_REGISTRY=docker.io

# Deployment Platforms
VERCEL_TOKEN=your_vercel_token
VERCEL_ORG_ID=your_org_id
VERCEL_PROJECT_ID=your_project_id

NETLIFY_SITE_ID=your_netlify_site_id
NETLIFY_AUTH_TOKEN=your_netlify_auth_token

# Monitoring and Analytics
SENTRY_DSN=your_sentry_dsn
SENTRY_AUTH_TOKEN=your_sentry_auth_token
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project

# Lighthouse CI
LHCI_GITHUB_APP_TOKEN=your_lhci_github_token
LHCI_SERVER_BASE_URL=https://your-lhci-server.com

# Security Scanning
SNYK_TOKEN=your_snyk_token
SNYK_ORG=your_snyk_org

# Testing
PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=0
PLAYWRIGHT_BROWSERS_PATH=node_modules/playwright/.local-browsers

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_PWA=false

# API Keys (if needed)
# VITE_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
# VITE_MAPBOX_TOKEN=pk.xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx