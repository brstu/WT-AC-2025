<svg width="1366" height="768" xmlns="http://www.w3.org/2000/svg">
  <rect width="1366" height="768" fill="#f5f5f5"/>
  
  <!-- Header -->
  <rect width="1366" height="80" fill="#333"/>
  <text x="40" y="35" font-family="Arial" font-size="24" fill="white">Справочник Open-Source проектов</text>
  <text x="40" y="65" font-family="Arial" font-size="14" fill="white">Главная</text>
  <text x="140" y="65" font-family="Arial" font-size="14" fill="white">Добавить проект</text>
  
  <!-- Back link -->
  <text x="40" y="120" font-family="Arial" font-size="14" fill="blue" text-decoration="underline">← Назад</text>
  
  <!-- Title -->
  <text x="40" y="160" font-family="Arial" font-size="28" fill="#333" font-weight="bold">Добавить новый проект</text>
  
  <!-- Form -->
  <g transform="translate(40, 190)">
    <!-- Field 1 -->
    <text y="15" font-family="Arial" font-size="14" fill="#333">Название *</text>
    <rect y="25" width="600" height="35" fill="white" stroke="#ddd" rx="4"/>
    
    <!-- Field 2 -->
    <text y="85" font-family="Arial" font-size="14" fill="#333">Описание *</text>
    <rect y="95" width="600" height="100" fill="white" stroke="#ddd" rx="4"/>
    
    <!-- Field 3 -->
    <text y="220" font-family="Arial" font-size="14" fill="#333">Автор *</text>
    <rect y="230" width="600" height="35" fill="white" stroke="#ddd" rx="4"/>
    
    <!-- Field 4 -->
    <text y="290" font-family="Arial" font-size="14" fill="#333">Количество звезд</text>
    <rect y="300" width="600" height="35" fill="white" stroke="#ddd" rx="4"/>
    
    <!-- Field 5 -->
    <text y="360" font-family="Arial" font-size="14" fill="#333">Язык программирования</text>
    <rect y="370" width="600" height="35" fill="white" stroke="#ddd" rx="4"/>
    
    <!-- Field 6 -->
    <text y="430" font-family="Arial" font-size="14" fill="#333">URL репозитория</text>
    <rect y="440" width="600" height="35" fill="white" stroke="#ddd" rx="4"/>
    
    <!-- Buttons -->
    <rect y="500" width="140" height="40" fill="#4CAF50" rx="4"/>
    <text x="70" y="525" font-family="Arial" font-size="16" fill="white" text-anchor="middle">Создать</text>
    
    <rect x="160" y="500" width="120" height="40" fill="#999" rx="4"/>
    <text x="220" y="525" font-family="Arial" font-size="16" fill="white" text-anchor="middle">Отмена</text>
  </g>
</svg>
